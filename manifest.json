{
    "manifest_version": 2,
    "name": "Hide Twitter Trends",
    "version": "0.8",
    "description": "Hide Twitter (Trending now, Who to follow) elements",
    "icons": {
        "128": "icon_128.png"
    },
    "content_scripts": [
        {
            "css": [
                "style.css"
            ],
            "matches": [
                "*://*.twitter.com/*"
            ]
        }
    ]
}
